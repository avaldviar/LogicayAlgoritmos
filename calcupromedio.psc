Proceso calprom
	Definir nota1, nota2, nota3, nota4, nota5, promedio,resultado Como real;
	Escribir "Digite la nota1";
	leer nota1;
	Escribir "Digite la nota2";
	leer nota2;
	Escribir "Digite la nota3";
	leer nota3;
	Escribir "Digite la nota4";
	leer nota4;
	Escribir "Digite la nota5";
	leer nota5;
	
	//Operaciones
	promedio<- (nota1 + nota2 +nota3 +nota4 +nota5)/5;
	
	Escribir "El promedio de las cinco asignaturas ", promedio;
	
	
FinProceso
